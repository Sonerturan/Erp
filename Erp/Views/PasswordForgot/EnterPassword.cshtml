    <div class="container-login100" style="background-image: url('~/images/bg-01.jpg');">
        <div class="wrap-login100 p-l-55 p-r-55 p-t-80 p-b-30">
            <div class="login100-form-title p-b-37">
                <a href=" @Url.Action("Index","Home")"><img src="~/images/logo13.png" /></a>
                <hr />
                <h3>@Request.Cookies["useruser"].Value</h3>
                <hr />
            </div>

            @using (Html.BeginForm())
            {
                @Html.AntiForgeryToken()

                <div class="form-horizontal">

                    <div class="form-group">
                        @Html.LabelFor(model => model.Password, htmlAttributes: new { @class = "control-label col-md-2" })
                        <i id="i" style="margin-top:16px;" class="fas fa-eye-slash" onclick="togglePassword()"></i>
                        <div class="col-md-9">
                            @Html.EditorFor(model => model.Password, new { htmlAttributes = new { @class = "wrap-input100 validate-input m-b-25" } })
                            @Html.ValidationMessageFor(model => model.Password, "", new { @class = "text-danger" })
                        </div>
                    </div>

                    <script>
                        function togglePassword() {
                            var element = document.getElementById('Password');
                            var element1 = document.getElementById('i');
                            element.type = (element.type == 'text' ? 'Password' : 'text');
                            element1.className = (element1.className == 'fas fa-eye' ? 'fas fa-eye-slash' : 'fas fa-eye');
                        }
                    </script>

                    <div class="form-group">
                        @Html.LabelFor(model => model.Passwordrepeat, htmlAttributes: new { @class = "control-label col-md-2" })
                        <i id="i1" style="margin-top:16px;" class="fas fa-eye-slash" onclick="togglePassword1()"></i>
                        <div class="col-md-9">
                            @Html.EditorFor(model => model.Passwordrepeat, new { htmlAttributes = new { @class = "wrap-input100 validate-input m-b-25" } })
                            @Html.ValidationMessageFor(model => model.Passwordrepeat, "", new { @class = "text-danger" })
                        </div>
                    </div>

                    <script>
                        function togglePassword1() {
                            var element = document.getElementById('Passwordrepeat');
                            var element1 = document.getElementById('i1');
                            element.type = (element.type == 'text' ? 'Password' : 'text');
                            element1.className = (element1.className == 'fas fa-eye' ? 'fas fa-eye-slash' : 'fas fa-eye');
                        }
                    </script>

                    <center>
                        <div class="form-group u">
                            <div class="col-md-offset-2 col-md-10 ">
                                <input id="sweetalert" type="submit" value="Kaydet" class="login100-form-btn" />
                            </div>
                        </div>
                    </center>
                </div>
            }

        </div>
